
{
  "log": {
    "loglevel": "warning"
  },
  "routing": {
    "domainStrategy": "AsIs",
    "rules": [
      {
        "type": "field",
        "ip": ["geoip:private"],
        "outboundTag": "block"
      }
    ]
  },
  "inbounds": [
    {
      "listen": "0.0.0.0",
      "port": 443,
      "protocol": "vless",
      "settings": {
        "clients": [
          { "id": "UUID_PHONE_1", "flow": "xtls-rprx-vision" },
          { "id": "UUID_PHONE_2", "flow": "xtls-rprx-vision" },
          { "id": "UUID_PC_1", "flow": "xtls-rprx-vision" },
          { "id": "UUID_PC_2", "flow": "xtls-rprx-vision" }
        ],
        "decryption": "none"
      },
      "streamSettings": {
        "network": "tcp",
        "security": "reality",
        "realitySettings": {
          "show": false,
          "dest": "www.microsoft.com:443",
          "xver": 0,
          "serverNames": ["www.microsoft.com"],
          "privateKey": "ТВОЙ_ПРИВАТНЫЙ_КЛЮЧ", // Тот, который выдал xray x25519
          "minClientVer": "",
          "maxClientVer": "",
          "maxTimeDiff": 0,
          "shortIds": ["b1", "c2", "d3"]
        }
      },
      "sniffing": {
        "enabled": true,
        "destOverride": ["http", "tls", "quic"]
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "tag": "direct" // Трафик уходит в интернет напрямую с этого сервера
    },
    {
      "protocol": "blackhole",
      "tag": "block"
    }
  ]
}
